#include <stdio.h>
int main() {
int arr[100];  
// Assuming a maximum of 100 elements
int size, i, sum = 0;
printf("Enter the size of the array: ");
scanf("%d", &size);
printf("Enter %d elements of the array:\n", size);
for (i = 0; i < size; i++) {
scanf("%d", &arr[i]);
    }
 // Calculate the sum of elements
 for (i = 0; i < size; i++) {
 sum += arr[i];
    }
 printf("Sum of elements in the array: %d\n", sum);
   return 0;
